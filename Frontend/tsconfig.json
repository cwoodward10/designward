{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@modules/*": ["src/modules"],
      "@layouts/*": ["src/layouts"],
      "@content/*": ["src/content"]
    }
  }
}